.admin-import-page{background:#f5f5f5;box-sizing:border-box;min-height:100vh;padding:20rpx}.admin-import-page .header{margin-bottom:20rpx;text-align:center}.admin-import-page .header .title{color:#333;font-size:18rpx;font-weight:700}.admin-import-page .logs{background:#fff;border-radius:8rpx;flex:1;margin-bottom:20rpx;max-height:400rpx;min-height:300rpx;padding:15rpx}.admin-import-page .logs .empty-log{color:#999;margin-top:100rpx;text-align:center}.admin-import-page .logs .log-item{color:#333;display:block;font-family:monospace;font-size:13rpx;line-height:1.8;margin-bottom:5rpx}.admin-import-page .progress-bar{background:#e0e0e0;border-radius:15rpx;height:30rpx;margin-bottom:20rpx;overflow:hidden;position:relative}.admin-import-page .progress-bar .progress-fill{background:linear-gradient(90deg,#1890ff,#52c41a);height:100%;left:0;position:absolute;top:0;transition:width .3s}.admin-import-page .progress-bar .progress-text{color:#333;font-size:12rpx;font-weight:700;left:50%;position:absolute;top:50%;transform:translate(-50%,-50%)}.admin-import-page .actions{display:flex;flex-direction:column;gap:15rpx}.admin-import-page .actions .btn{border:none;border-radius:8rpx;font-size:16rpx}.admin-import-page .actions .btn.btn-check{background:#1890ff;color:#fff}.admin-import-page .actions .btn.btn-import{background:#52c41a;color:#fff}.admin-import-page .actions .btn.btn-clear{background:#ff4d4f;color:#fff}.admin-import-page .actions .btn[disabled]{background:#d9d9d9;color:#999}